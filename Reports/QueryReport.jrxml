	<queryString>
		<![CDATA[SELECT
     Employees."FIRST_NAME" AS Employees_FIRST_NAME,
     Employees."CALL_NAME" AS Employees_CALL_NAME,
     Employees."INITIALS" AS Employees_INITIALS,
     UnAndPw."USER_NAME" AS UnAndPw_USER_NAME,
     UserPrivilegeForModules."MODULE_CODE" AS UserPrivilegeForModules_MODULE_CODE,
     UserPrivilegeForModules."EMPLOYEE_CODE" AS UserPrivilegeForModules_EMPLOYEE_CODE,
     Modules."MODULE_NAME" AS Modules_MODULE_NAME,
     Modules."VERSION" AS Modules_VERSION,
     Employees."ACTIVE" AS Employees_ACTIVE
FROM
     "dbo"."UnAndPw" UnAndPw INNER JOIN "dbo"."Employees" Employees ON UnAndPw."EMPLOYEE_CODE" = Employees."EMPLOYEE_CODE"
     INNER JOIN "dbo"."UserPrivilegeForModules" UserPrivilegeForModules ON Employees."EMPLOYEE_CODE" = UserPrivilegeForModules."EMPLOYEE_CODE"
     INNER JOIN "dbo"."Modules" Modules ON UserPrivilegeForModules."MODULE_CODE" = Modules."MODULE_CODE"
WHERE
     Employees."ACTIVE" = 'Yes'
ORDER BY
     Employees."CALL_NAME" ASC]]>
	</queryString>
